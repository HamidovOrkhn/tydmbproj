@{
}
@model List<TCYDMWebApp.DTO.CountryDTO>
@foreach (var item in Model)
{
    if (Convert.ToInt32(Context.Request.Cookies["cId"]) == item.Id)
    {
        <option selected value=@Convert.ToUInt32(item.Id)>@item.Name</option>
    }
    else
    {
        <option value=@Convert.ToUInt32(item.Id)>@item.Name</option>
    }

}
